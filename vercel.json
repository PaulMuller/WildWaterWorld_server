{
	"builds": [
		{
			"src": "./index.js",
			"use": "@vercel/node"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{"key": "origin","value": "https://wild-water-world-client-side.vercel.app"},
				{"key": "Access-Control-Allow-Origin", "value": "https://wild-water-world-client-side.vercel.app/" },
				{"key": "Access-Control-Allow-Methods", "value": "POST, GET" }	
			]
		}
	]
}